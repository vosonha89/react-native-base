{
  "name": "react-native-base",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 5 --ignore-pattern \"framework/**\" --ignore-pattern \"*.js\"",
    "start": "npm run lint && react-native start --reset-cache",
    "regenerate-platform": "npx react-native eject",
    "start:debug": "npm run lint && npx react-native start --reset-cache",
    "build:prepare": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android:build-debug": "npm run build:prepare && cd android && ./gradlew assembleDebug",
    "android:permit": "cd android && chmod +x ./gradlew",
    "android:installdebug": "npm run build:debug && cd android && adb install app/build/outputs/apk/debug/app-debug.apk",
    "devtools": "react-devtools",
    "link": "npx react-native-asset",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "7.1.17",
    "@react-navigation/native-stack": "7.3.26",
    "one-frontend-framework": "^0.0.6",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.14.1",
    "reflect-metadata": "^0.2.1",
    "tsyringe": "^4.10.0",
    "@react-native/new-app-screen": "0.81.4",
    "react-native-safe-area-context": "^5.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.24.1",
    "@babel/plugin-transform-class-properties": "^7.24.1",
    "@babel/plugin-transform-class-static-block": "^7.24.4",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@eslint/js": "^9.2.0",
    "@react-native-community/cli": "20.0.2",
    "@react-native-community/cli-platform-android": "20.0.2",
    "@react-native-community/cli-platform-ios": "20.0.2",
    "@react-native/babel-preset": "0.81.4",
    "@react-native/eslint-config": "0.81.4",
    "@react-native/metro-config": "0.81.4",
    "@react-native/typescript-config": "0.81.4",
    "@types/react": "19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@types/jest": "^29.5.13",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "eslint-plugin-react": "^7.34.1",
    "globals": "^15.2.0",
    "react-native-eject": "^0.2.0",
    "react-test-renderer": "19.1.0",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.43.0"
  },
  "engines": {
    "node": ">=18"
  }
}