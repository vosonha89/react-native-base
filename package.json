{
  "name": "react-native-base",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 5 --ignore-pattern \"framework/**\" --ignore-pattern \"*.js\"",
    "start": "npm run lint && react-native start --experimental-debugger --reset-cache",
    "regenerate-platform": "npx react-native eject",
    "start:debug": "npm run lint && npx react-native start --experimental-debugger --reset-cache",
    "build:debug": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && gradlew.bat assembleDebug",
    "install:debug": "npm run build:debug && cd android && adb install app/build/outputs/apk/debug/app-debug.apk",
    "devtools": "react-devtools",
    "link": "npx react-native-asset",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "one-frontend-framework": "^0.0.6",
    "react": "18.2.0",
    "react-native": "0.74.1",
    "react-native-config": "^1.5.1",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-webview": "^11.23.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.24.1",
    "@babel/plugin-transform-class-properties": "^7.24.1",
    "@babel/plugin-transform-class-static-block": "^7.24.4",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@eslint/js": "^9.2.0",
    "@react-native-community/cli": "^13.5.1",
    "@react-native/babel-preset": "0.74.83",
    "@react-native/eslint-config": "0.74.83",
    "@react-native/metro-config": "0.74.83",
    "@react-native/typescript-config": "0.74.83",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "babel-jest": "^29.6.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "globals": "^15.2.0",
    "jest": "^29.6.3",
    "react-native-eject": "^0.2.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4",
    "typescript-eslint": "^7.8.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
